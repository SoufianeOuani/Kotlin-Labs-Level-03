//Fibonacci using Loop

fun printFibonacciSeriesOf10UsingLoop()
{
    var number1 = 1
    var number2 = 0
    var fibonacciSeries = 0

    print("$number1\t")
    for (i in 1 until 10)
    {
        fibonacciSeries = number1 + number2
        print("$fibonacciSeries\t")
        number2 = number1
        number1 = fibonacciSeries
    }

}

fun main()
{
    printFibonacciSeriesOf10UsingLoop()
}
